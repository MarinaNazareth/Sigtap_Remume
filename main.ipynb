   "source": [
    "#definicao dos caracteres especiais que devem ser escapados do regex\n",
    "\n",
    "def escape_special_chars(string):\n",
    "    special_chars = ['\\\\', '^', '$', '|', '?', '*', '(', ')', '[', ']', '{', '}', ':']\n",
    "\n",
    "    for char in special_chars:\n",
    "        string = string.replace(char, '\\\\' + char)\n",
    "    \n",
    "    return string"
   ]
  },
